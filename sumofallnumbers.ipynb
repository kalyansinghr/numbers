   "source": [
    "# Write a Python function to sum all the numbers in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "313aa2e4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "20\n"
     ]
    }
   ],
   "source": [
    "def sum(numbers): #DECLARING THE FUNCTION\n",
    "    value = 0\n",
    "    for x in numbers:\n",
    "        value += x\n",
    "    return value   #RETURNING TRHE FUNCTION\n",
    "print(sum((8, 2, 3, 0, 7)))"
   ]
  }
